{
  "manifest_version": 2,
  "name": "Miner for drill",
  "description": "Captures Pega Academy quiz answers and transforms into Drill friendly text",
  "version": "1.0",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html",
    "default_title": "Export quize questions and answers!"
  },
  "permissions": [
    "activeTab"
  ]
}